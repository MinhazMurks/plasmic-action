{
  "name": "plasmic-action",
  "version": "0.0.0",
  "private": true,
  "description": "Plasmic Sync and Build Action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plasmicapp/plasmic-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "plasmic"
  ],
  "author": "Plasmic <ops@plasmic.app>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@sentry/node": "^6.2.5",
    "@sentry/tracing": "^6.2.5",
    "nanoid": "^3.1.22"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@vercel/ncc": "^0.27.0",
    "js-yaml": "^4.0.0",
    "prettier": "2.2.1",
    "typescript": "^4.2.3"
  }
}
